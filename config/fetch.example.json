{
    "servers": {
	"input": {
	    "host": "127.0.0.1",
	    "port": 5557
	},
	"output": {
	    "host": "127.0.0.1",
	    "port": 5558
	},
	"failed": {
	    "host": "127.0.0.1",
	    "port": 5559
	}
    },

    "requests": {

	"activities": {
	    "id": {
		"name": "channelId",
		"num_per_request": 1
	    },
	    "args": {
		"part": ["snippet"],
		"fields": "nextPageToken,items(snippet(publishedAt,type))"
	    }
	},

	"playlistItems": {
	    "id": {
		"name": "playlistId",
		"num_per_request": 1
	    },
	    "args": {
		"part": [
		    "id",
		    "snippet",
		    "status"
		],
		"fields": "nextPageToken,items(status,snippet(channelId,title,resourceId,playlistId,publishedAt,channelTitle))"
	    }
	},

	"videos": {
	    "id": {
		"name": "id",
		"num_per_request": 50
	    },
            "args": {
		"part": [
		    "contentDetails",
		    "snippet",
		    "statistics",
		    "status",
		    "topicDetails"
		],
		"fields": "items(kind,id,status,statistics,topicDetails,contentDetails(duration,licensedContent),snippet(publishedAt,channelId,title,categoryId,liveBroadcastContent))"
	    }
	},

	"channels": {
	    "id": {
		"name": "id",
		"num_per_request": 50
	    },
            "args": {
		"unused_part": [
		    "contentDetails",
		    "topicDetails"
		],
		"part": [
		    "snippet",
		    "statistics",
		    "status"
		],
                "fields": "items(kind,id,status,statistics,topicDetails,contentDetails,snippet(title,publishedAt))"
	    }
	}
    },

    "concurrency": {
	"batches": {
	    "num_requests": 50,
	    "interval_ms": 250
	},
	"sleep_ms": 1000
    }
}
