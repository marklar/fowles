{
    "servers": {
	"input": {
	    "host": "127.0.0.1",
	    "port": 1337
	},
	"output": {
	    "host": "127.0.0.1",
	    "port": 1338
	},
	"failed": {
	    "host": "127.0.0.1",
	    "port": 1339
	}
    },

    "concurrency": {
	"batch_size": 5,
	"interval_ms": 250
    },

    "values": {
	"affiliation": {
            "regexp": "\"content_owner_name\": \"([^\"]*)\"",
	    "match_index": 1
	},
	"preroll": {
	    "regexp": "\"ad_preroll\": \"([^\"]*)\"",
	    "match_index": 1
	},
	"premium_ads" {
	    "regexp": "\"show_instream\": (true|false)",
	    "match_index": 1
	},
	"show_afv": {
	    "regexp": "\"show_afv\": (true|false)",
	    "match_index": 1
	},
	"afv_user_id": {
	    "regexp": ",afv_user_id_([^,]*),",
	    "match_index": 1
	},
	"owner_id": {
	    "regexp": "\"owner_id\": \"([^\"]*)\"",
	    "match_index": 1
	},
	"pltype": {
	    "regexp": "\"pltype\": \"([^\"]*)\"",
	    "match_index": 1
	},
	"adsense1_number": {
	    "regexp": "\"ad_video_pub_id\": \"ca-pub-(\\d*)\"",
	    "match_index": 1
	},
	"adsense2_number": {
	    "regexp": "\"ad_host\": \"ca-host-pub-(\\d*)\"",
	    "match_index": 1
	},
	"video_duration": {
	    "regexp": "<meta itemprop=\"duration\" content=\"([^\"]*)\">",
	    "match_index": 1
	},
	"afv_video_min_cpm": {
	    "regexp": "\"afv_video_min_cpm\": (\\d*),",
	    "match_index": 1
	},
	"trueview": {
	    "regexp": "\"trueview\": (true|false)",
	    "match_index": 1
	},
	"unavailable": {
	    "regexp": "id=\"unavailable-message\"\\s*[^>]*>\\s*([^<]+?)\\s*</h1",
	    "match_index": 1
	},
	"family_friendly": {
	    "regexp": "itemprop=\"isFamilyFriendly\" content=\"(True|False)\"",
	    "match_index": 1
	},
	"ptk": {
	    "regexp": "\"ptk\": \"([^\"]*)\"",
	    "match_index": 1
	},
	"allowed_ads": {
	    "regexp": "\"allowed_ads\": \\[([^\\[]*)\\]",
	    "match_index": 1
	},
	"skipable": {
	    "regexp": "\"skipable\": \"([^\"]*)\"",
	    "match_index": 1
	},
	"as_launched_in_country": {
	    "regexp": "\"as_launched_in_country\": \"([^\"]*)\"",
	    "match_index": 1
	},
	"eow_category": {
	    "regexp": "<p id=\"eow-category\"><a [^>]*>([^<]*)</a>",
	    "match_index": 1
	},
	"ytp_author_attribution": {
	    "regexp": "YTP_AUTHOR_ATTRIBUTION[^>]*>([^<]*)<",
	    "match_index": 1
	},
	"keywords": {
	    "regexp": "<meta name=\"keywords\" content=\"([^\"]*)\">",
	    "match_index": 1
	}
    }
}
